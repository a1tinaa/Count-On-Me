<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.countdowntoTHPTQG">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher2"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher2_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.example.countdowntoTHPTQG.MainActivity"></activity>

        <meta-data
            android:name="com.google.android.actions"
            android:resource="@xml/rounded_corner" />

        <activity android:name="com.example.countdowntoTHPTQG.IntroActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
        <receiver android:name="com.example.countdowntoTHPTQG.NotificationReceiver"/>
    </application>

</manifest>